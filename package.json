    "start": "next start",
    "lint": "next lint",
    "husky": "husky install",
    "husky:pre-commit": "husky add .husky/pre-commit \"npm run pre-commit\"",
    "pre-commit": "yarn tsc --pretty --noEmit && yarn eslint . --fix --max-warnings=0",
    "tree-add-lib": "git subtree add --prefix src/lib https://github.com/SocialMeDev/tec-impacto-library-web.git main --squash",
    "tree-pull-lib": "git subtree pull --prefix src/lib https://github.com/SocialMeDev/tec-impacto-library-web.git main --squash",
    "tree-push-lib": "git subtree push --prefix src/lib https://github.com/SocialMeDev/tec-impacto-library-web.git main",
    "init:setup": "yarn && yarn husky && yarn husky:pre-commit"
  },
